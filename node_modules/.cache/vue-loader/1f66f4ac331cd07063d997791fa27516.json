n-12\">\r\n              <v-window v-model=\"step\">\r\n                <v-window-item :value=\"1\">\r\n                  <v-row>\r\n                    <v-col cols=\"12\" md=\"8\">\r\n                        <v-card-text class=\"mt-12\">\r\n                            <h4 class=\"text-center mt-4\">Login</h4>\r\n                            <v-form>\r\n                                <v-text-field\r\n                                label=\"correo\"\r\n                                name=\"Email\"\r\n                                type=\"text\"\r\n                                color=\"teal accent-3\"\r\n                                v-model=\"emailRecovery\"\r\n                                />\r\n                            <v-text-field\r\n                            id=\"password\"\r\n                            label=\"Password\"\r\n                            name=\"password\"\r\n                            type=\"password\"\r\n                            color=\"teal accent-3\"\r\n                            v-model=\"emailLogin\"\r\n                          />\r\n                        </v-form>\r\n                        <h3 class=\"text-center mt-4\">Olvidaste tu contrase√±a?</h3>\r\n                        <div class=\"text-center mt-3\">\r\n                            <v-btn rounded color=\"teal accent-3\" dark @click=\"step--\">Recuperar password</v-btn>\r\n                        </div>\r\n                      </v-card-text>\r\n                      <div class=\"text-center mt-3\">\r\n                        <v-btn rounded color=\"teal accent-3\" dark>Iniciar sesion</v-btn>\r\n                      </div>\r\n                    </v-col>\r\n                    <v-col cols=\"12\" md=\"4\" class=\"teal accent-3\">\r\n                      <v-card-text class=\"white--text mt-12\">\r\n                        <h1 class=\"text-center display-1\">Bienvenido</h1>\r\n                        <h5\r\n                          class=\"text-center\"\r\n                        >Ingresa ya para poder dara tus servicios a las personas</h5>\r\n                      </v-card-text>\r\n                      <div class=\"text-center\">\r\n                        <v-btn rounded outlined dark @click=\"step++\">Registrate</v-btn>\r\n                      </div>\r\n                    </v-col>\r\n                  </v-row>\r\n                </v-window-item>\r\n                <v-window-item :value=\"2\">\r\n                  <v-row class=\"fill-height\">\r\n                    <v-col cols=\"12\" md=\"4\" class=\"teal accent-3\">\r\n                      <v-card-text class=\"white--text mt-12\">\r\n                        <h1 class=\"text-center display-1\">Bienvenido</h1>\r\n                        <h5\r\n                          class=\"text-center\"\r\n                        >Mantente conectado con tus clientes para poder generar mas ingresos haciendo lo que te gusta</h5>\r\n                      </v-card-text>\r\n                      <div class=\"text-center\">\r\n                        <v-btn rounded outlined dark @click=\"step--\">Iniciar sesion</v-btn>\r\n                      </div>\r\n                    </v-col>\r\n\r\n                    <v-col cols=\"12\" md=\"8\">\r\n                      <v-card-text class=\"mt-12\">\r\n                        <h1 class=\"text-center display-2 teal--text text--accent-3\">Crea una cuenta</h1>\r\n                        <h4 class=\"text-center mt-4\">Asegure su correo para registrarse</h4>\r\n                        <v-form>\r\n                          <v-text-field\r\n                            label=\"Nombre\"\r\n                            name=\"Name\"\r\n                            type=\"text\"\r\n                            color=\"teal accent-3\"\r\n                            v-model=\"firstname\"\r\n                          />\r\n                          <v-text-field\r\n                            label=\"Apellido Paterno\"\r\n                            name=\"Name\"\r\n                            type=\"text\"\r\n                            color=\"teal accent-3\"\r\n                            v-model=\"lastname\"\r\n                          />\r\n                          <v-text-field\r\n                            label=\"Apellido Materno\"\r\n                            name=\"Name\"\r\n                            type=\"text\"\r\n                            color=\"teal accent-3\"\r\n                            v-model=\"secondname\"\r\n                          />\r\n                          <v-select\r\n                          :items=\"dataSelectSexo\"\r\n                          v-model=\"dbSelect\"\r\n                          item-text=\"strCampo\"\r\n                          item-value=\"strCampo\"\r\n                          label=\"Sexo\"\r\n                          />\r\n                          <v-text-field\r\n                            label=\"Email\"\r\n                            name=\"Email\"\r\n                            type=\"text\"\r\n                            color=\"teal accent-3\"\r\n                            v-model=\"email\"\r\n                          />\r\n                          <v-text-field\r\n                            id=\"password\"\r\n                            label=\"Password\"\r\n                            name=\"password\"\r\n                            type=\"password\"\r\n                            color=\"teal accent-3\"\r\n                            v-model=\"password\"\r\n                          />\r\n                        </v-form>\r\n                      </v-card-text>\r\n                      <div class=\"text-center mt-n5\">\r\n                        <v-btn rounded color=\"teal accent-3\" dark>Registrar</v-btn>\r\n                      </div>\r\n                    </v-col>\r\n                  </v-row>\r\n                </v-window-item>\r\n                <v-window-item :value=\"0\">\r\n                  <v-row class=\"fill-height\">\r\n                    <v-col cols=\"12\" md=\"4\" class=\"teal accent-3\">\r\n                      <v-card-text class=\"white--text mt-12\">\r\n                        <h1 class=\"text-center display-1\">Cuidado, no compartir email</h1>\r\n                        <h5\r\n                          class=\"text-center\"\r\n                        >Mantente conectado con tus clientes para poder generar mas ingresos haciendo lo que te gusta</h5>\r\n                      </v-card-text>\r\n                      <div class=\"text-center\">\r\n                        <v-btn rounded outlined dark @click=\"step++\">Volver a registro</v-btn>\r\n                      </div>\r\n                    </v-col>\r\n\r\n                    <v-col cols=\"12\" md=\"8\">\r\n                      <v-card-text class=\"mt-12\">\r\n                        <v-form>\r\n                          <v-text-field\r\n                            label=\"Email\"\r\n                            name=\"Email\"\r\n                            type=\"text\"\r\n                            color=\"teal accent-3\"\r\n                            v-model=\"emailRecovery\" \r\n                          />\r\n                        </v-form>\r\n                      </v-card-text>\r\n                      <div class=\"text-center mt-n5\">\r\n                        <v-btn rounded color=\"teal accent-3\" @click=\"Recovery()\" dark>Recuperar</v-btn>\r\n                      </div>\r\n                    </v-col>\r\n                  </v-row>\r\n                </v-window-item>\r\n              </v-window>\r\n            </v-card>\r\n          </v-col>\r\n        </v-row>\r\n      </v-container>\r\n    </v-content>\r\n  </v-app>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from \"axios\";\r\nexport default {\r\n  data: () => ({\r\n    step: 1,\r\n    emailRecovery:'',\r\n    emailLogin:'',\r\n    passwordLogin:'',\r\n    userRegister:{\r\n        firstname: \"\",\r\n        lastname: \"\",\r\n        secondname: \"\",\r\n        Sex: \"\",\r\n        email: \"\",\r\n        password: \"\",\r\n        token: \"\",\r\n        catRolId: 2,\r\n    },\r\n    dataSelectSexo:\r\n    [\r\n      {id:1,strCampo:\"Hombre\"},\r\n      {id:2,strCampo:\"Mujer\"}\r\n    ],\r\n    dbSelect:\"\"\r\n  }),\r\n  props: {\r\n    source: String\r\n  },\r\n  methods: {\r\n    regreso(){\r\n                this.$router.push(`/pages/alerts`);\r\n            },\r\n      Registration() {\r\n      //aqui debe encriptar el token con sha256\r\n      this.userRegister.Sex = this.dbSelect;\r\n      axios.get(\"https://apipet18301044.herokuapp.com/users/\"+this.userRegister).then((result) => {\r\n      this.result = result.data;\r\n      })\r\n    },\r\n    Login() {\r\n      axios.get(\"https://apipet18301044.herokuapp.com/login/\"+this.emailLogin+\"/\"+this.passwordLogin).then((result) => {\r\n      this.result = result.data;\r\n      })\r\n    },\r\n    Recovery() {\r\n      axios.get(\"https://apipet18301044.herokuapp.com/recuperarPassword/\"+this.emailRecovery).then((result) => {\r\n      this.result = result.data;\r\n      })\r\n    },\r\n  },\r\n  \r\n};\r\n</script>"]}]}